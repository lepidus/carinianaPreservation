msgid "plugins.generic.carinianaPreservation.displayName"
msgstr "Preservación Cariniana"

msgid "plugins.generic.carinianaPreservation.description"
msgstr "Este módulo permite conservar revistas brasileñas publicadas en OJS en la red Cariniana"

msgid "plugins.generic.carinianaPreservation.settings.extraCopyEmail"
msgstr "Copia adicional de correo electrónico"

msgid "plugins.generic.carinianaPreservation.settings.extraCopyEmail.description"
msgstr "Esta dirección se colocará como copia en el correo electrónico de envío de la revista para su preservación"

msgid "plugins.generic.carinianaPreservation.settings.responsabilityStatement"
msgstr "Condiciones de responsabilidad"

msgid "plugins.generic.carinianaPreservation.settings.responsabilityStatement.alreadySent.notice"
msgstr "El archivo completo de los términos de responsabilidad <strong>ya fue enviado anteriormente</strong> y está guardado en el sistema. Se eliminará después de la primera preservación."

msgid "plugins.generic.carinianaPreservation.settings.responsabilityStatement.alreadySent.instructions"
msgstr "Si desea reemplazar el archivo enviado anteriormente, descargue, complete y firme el documento de los <a href=\"{$downloadStatementUrl}\">términos de responsabilidad</a> y luego adjunte el archivo en el campo a continuación."

msgid "plugins.generic.carinianaPreservation.settings.responsabilityStatement.preserved.notice"
msgstr "El archivo firmado de los términos de responsabilidad se envió con la primera solicitud de preservación y luego se eliminó de forma segura. No se requiere nada más aquí."

msgid "plugins.generic.carinianaPreservation.settings.responsabilityStatement.description"
msgstr "Descargue, complete y firme los <a href=\"{$downloadStatementUrl}\">términos de responsabilidad</a> y adjunte el archivo abajo. Después de la primera preservación el archivo será eliminado (minimización de datos)."

msgid "plugins.generic.carinianaPreservation.headers.publisherOrInstitution"
msgstr "Editor/Institución"

msgid "plugins.generic.carinianaPreservation.headers.title"
msgstr "Título"

msgid "plugins.generic.carinianaPreservation.headers.issn"
msgstr "ISSN"

msgid "plugins.generic.carinianaPreservation.headers.eIssn"
msgstr "eISSN"

msgid "plugins.generic.carinianaPreservation.headers.baseUrl"
msgstr "URL base"

msgid "plugins.generic.carinianaPreservation.headers.journalPath"
msgstr "Identificador de la revista"

msgid "plugins.generic.carinianaPreservation.headers.availableYears"
msgstr "Años disponibles"

msgid "plugins.generic.carinianaPreservation.headers.issuesVolumes"
msgstr "Volumen de los números"

msgid "plugins.generic.carinianaPreservation.headers.notesAndComments"
msgstr "Notas y comentarios"

msgid "plugins.generic.carinianaPreservation.preservationEmail.subject"
msgstr "{$journalAcronym}: solicitud de inclusión en Cariniana."

msgid "plugins.generic.carinianaPreservation.preservationEmail.body"
msgstr "La identificación de contenido de la revista {$journalAcronym} se adjunta a este correo electrónico"

msgid "plugins.generic.carinianaPreservation.preservationUpdateEmail.subject"
msgstr "{$journalAcronym}: actualización de datos."

msgid "plugins.generic.carinianaPreservation.preservationUpdateEmail.body"
msgstr "La actualización de contenido de la revista {$journalAcronym} se adjunta a este correo electrónico"

msgid "plugins.generic.carinianaPreservation.preservationSubmission"
msgstr "Envío para preservación"

msgid "plugins.generic.carinianaPreservation.preservationSubmission.notesAndComments.description"
msgstr "Si está interesado, inserte aquí alguna información relevante sobre la publicación"

msgid "plugins.generic.carinianaPreservation.preservationSubmission.emailAddresses"
msgstr ""
"El correo electrónico con los datos de la revista será enviado a la dirección de correo electrónico de la Rede Cariniana (cariniana-periodicos@ibict.br), "
"con copia a la(s) dirección(es) de correo electrónico: {$emailCopies}"

msgid "plugins.generic.carinianaPreservation.preservationSubmission.first.instruction"
msgstr "Haga clic en \"Enviar\" para realizar la primera presentación de esta revista para la preservación digital por la Red Cariniana."

msgid "plugins.generic.carinianaPreservation.preservationSubmission.update.instruction"
msgstr "Haga clic en \"Enviar\" para enviar una actualización con los cambios desde la última preservación."

msgid "plugins.generic.carinianaPreservation.preservationSubmission.missingRequirements"
msgstr "No se ha podido realizar el envío de la revista. Los siguientes datos deben ser llenados: {$missingRequirements}"

msgid "plugins.generic.carinianaPreservation.preservationSubmission.missingResponsabilityStatement"
msgstr "Archivo de descargo de responsabilidad no enviado. Accede a la configuración del plugin para enviar"

msgid "plugins.generic.carinianaPreservation.preservationSubmission.noChanges"
msgstr "No se detectaron cambios desde la última preservación. Nada que actualizar."

msgid "plugins.generic.carinianaPreservation.preservationSubmission.lastPreservation"
msgstr "Última preservación realizada el:"

msgid "plugins.generic.carinianaPreservation.preservationSubmission.lockssDisabled"
msgstr "LOCKSS no está habilitado en Distribución > Archivo. <a href=\"{$lockssSettingsUrl}\" target=\"_blank\">Abra la configuración</a> para habilitarlo y permitir el envío."

msgid "plugins.generic.carinianaPreservation.updateChecker"
msgstr "Verificación de cambios en la revista para actualización de preservación"

msgid "plugins.generic.carinianaPreservation.updateChecker.notPreserved"
msgstr "{$journalName} aún no ha sido preservada."

msgid "plugins.generic.carinianaPreservation.updateChecker.updateEmailSent"
msgstr "{$journalName} tuvo cambios desde la última preservación. El correo electrónico de actualización fue enviado a la Red Cariniana."

msgid "plugins.generic.carinianaPreservation.updateChecker.updateEmailFailed"
msgstr "{$journalName} tuvo cambios desde la última preservación. Pero ocurrió un error al enviar el correo electrónico de actualización: {$error}"

msgid "plugins.generic.carinianaPreservation.updateChecker.noChanges"
msgstr "{$journalName} no tuvo cambios desde la última preservación."

msgid "plugins.generic.carinianaPreservation.updateChecker.lockssDisabled"
msgstr "{$journalName}: verificación ignorada porque LOCKSS está deshabilitado."
